{
  "manifest_version": 3,
  "name": "Todo List Extension",
  "version": "1.0",
  "description": "A simple todo list extension with time tracking",
  "permissions": [
    "activeTab",
    "scripting",
    "storage"
  ],
  "host_permissions": [
    "<all_urls>"
  ],
  "action": {
    "default_popup": "popup.html",
    "default_icon": {
      "16": "images/16.png",
      "128": "images/128.png"
    }
  }
}